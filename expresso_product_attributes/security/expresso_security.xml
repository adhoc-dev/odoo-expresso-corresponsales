<openerp>
<data noupdate="0">


    <record id="expresso_product_attributes.expresso_grupo_expresso" model="res.groups">
        <field name="name">Expresso / Expresso</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <!--<field name="implied_ids" eval="[(4, ref('base.group_system'))]"/>-->
    </record>
    
    <record id="expresso_product_attributes.expresso_grupo_corresponsales" model="res.groups">
        <field name="name">Expresso / Corresponsales</field>
        <field name="is_portal" eval="True"/>
        <field name="implied_ids" eval="[(4,ref('base.group_portal'))]"/>
        <!--<field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>-->
        <!--<field name="implied_ids" eval="[(4, ref('base.group_system'))]"/>-->
    </record>


    <record id="partner_expresso" model="res.partner">
            <field name="name">Expresso User</field>
            <!--<field name="company_id" ref="main_company"/>-->
            <field name="customer" eval="False"/>
            <field name="email">expresso@yourcompany.example.com</field>
            <field name="street">Example</field>
            <field name="city">Argentina</field>
            <field name="zip">5101</field>
            <!--<field name="country_id" ref="ar"/>-->
    </record>
    <record id="user_expresso" model="res.users">
            <field name="partner_id" ref="expresso_product_attributes.partner_expresso"/>
            <field name="login">expresso</field>
            <field name="password">expresso</field>
            <field name="signature">Expresso</field>
            <!--<field name="company_id" ref="main_company"/>-->
            <!--<field name="groups_id" eval="[(6,0,[ref('base.group_user'),ref('base.group_portal'), ref('base.group_partner_manager'),ref('expresso_product_attributes.expresso_grupo_expresso')])]"/>-->
            <field name="groups_id" eval="[(6,0,[ref('base.group_partner_manager'),ref('expresso_product_attributes.expresso_grupo_expresso')])]"/>
    </record>


    <!--USER CORRESPONSAL-->
     <record id="partner_corresponsal" model="res.partner">
            <field name="name">Demo Corresponsal</field>
            <field name="email">demo.corresponsal@yourcompany.example.com</field>
            <field name="supplier" eval="False"/>
            <field name="customer" eval="True"/>
            <field name="notify_email">none</field>
            <field name="city">Argentina</field>
            <field name="zip">4683</field>
        </record>

        <record id="demo_corresponsal_user" model="res.users" context="{'no_reset_password': True}">
            <field name="partner_id" ref="partner_corresponsal"/>
            <field name="login">corresponsal</field>
            <field name="password">corresponsal</field>
            <field name="signature">Corresponsal</field>
            <!-- Avoid auto-including this user in any default group -->
            <field name="groups_id" eval="[(5,)]"/>
        </record>

        <!-- Add the demo user to the portal (and therefore to the portal member group) -->
        <record id="expresso_product_attributes.expresso_grupo_corresponsales" model="res.groups">
            <field name="users" eval="[(4,ref('demo_corresponsal_user'))]"/>
        </record>





